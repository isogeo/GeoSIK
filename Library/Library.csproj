<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <ProductVersion>8.0.30703</ProductVersion>
    <RootNamespace>GeoSik</RootNamespace>
    <AssemblyName>GeoSik</AssemblyName>
    <TargetFramework>net462</TargetFramework>
